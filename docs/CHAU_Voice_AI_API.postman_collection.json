  "item": [
    {
      "name": "Partner Integration",
      "description": "Single endpoint for external partners to trigger AI calls",
      "item": [
        {
          "name": "Partner Call - Trigger AI Call",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"campaign_id\": \"YOUR_CAMPAIGN_ID\",\n  \"phone\": \"+14155551234\",\n  \"firstname\": \"John\",\n  \"lastname\": \"Doe\",\n  \"email\": \"john@example.com\",\n  \"product\": \"Enterprise Plan\",\n  \"notes\": \"Interested in demo, located in San Francisco\",\n  \"lead_source\": \"partner_api\",\n  \"callback_url\": \"https://your-webhook.com/callback\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/partner/call",
              "host": ["{{baseUrl}}"],
              "path": ["api", "partner", "call"]
            },
            "description": "Create/find lead, add to campaign, and initiate AI call in one request. The notes field is flexible - use it for any custom context."
          }
        }
      ]
    },
